{
    "version": 2,
    "name": "blog.spa",
    "scope": "lampheads",
    "builds": [
        { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "public" } },
        { "src": "public/**", "use": "@now/static"}
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}